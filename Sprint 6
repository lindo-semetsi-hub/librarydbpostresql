SELECT * FROM books 
WHERE published_year > 1950;

SELECT * FROM authors
WHERE nationality = 'American';

UPDATE books
SET available = TRUE;

SELECT * FROM books 
WHERE available = TRUE AND published_year > 1950;

SELECT * FROM authors 
WHERE name ILIKE '%George%';

UPDATE books
SET published_year = published_year + 1
WHERE published_year = 1869;
